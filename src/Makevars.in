PKG_CFLAGS=@PKG_CFLAGS@
PKG_CPPFLAGS=@PKG_CPPFLAGS@ -DNOUNDERLAPACK -DNOUNDERBLAS -DNOSHORTS
PKG_LIBS=@PKG_LIBS@

$(SHLIB): @CSDP_TS@

Csdp.ts:
	(cd Csdp/lib && \
	($(MAKE) libsdp.a CC="$(CC)" CFLAGS="$(CFLAGS) $(CPICFLAGS) -DNOUNDERLAPACK -DNOUNDERBLAS -DNOSHORTS -I../include" AR="$(AR)" RANLIB="$(RANLIB)") && \
	touch $@)
